port Runnable {
  function start() : Nil
  function stop() : Nil
  machine M {
    state Stopped {
      start() = setNextState(Started)
    }

    state Started {
      stop() = setNextState(Stopped)
    }
  }
}

@runtime(.MultiThreaded)
external component RunnableBase {
  val client : Provided<Runnable>
}
